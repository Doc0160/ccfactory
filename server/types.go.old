package main

import "encoding/json"

type Login struct {
	Client string `json:"client"`
}

type Request struct {
	Id   int    `json:"id"`
	Type string `json:"type"`
	Args []any  `json:"args"`
}

type Response struct {
	Id     int             `json:"id"`
	Result json.RawMessage `json:"result"`
	Error  string          `json:"error"`
}
